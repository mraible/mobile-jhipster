{
  "name": "flickr-2",
  "version": "0.0.1",
  "scripts": {
    "android": "expo start --android",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "eject": "expo eject",
    "postinstall": "patch-package",
    "ios": "expo start --ios",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{,.,**/,.jhipster/**/}*.{md,json,yml,js,ts,tsx}\"",
    "publish": "expo publish",
    "start": "expo start",
    "test": "jest",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "web": "expo start --web"
  },
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./test/setup"
    ],
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-masked-view/masked-view": "0.2.5",
    "@react-native-picker/picker": "1.16.1",
    "@react-navigation/devtools": "5.1.22",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "apisauce": "2.1.1",
    "expo": "~42.0.1",
    "expo-auth-session": "~3.2.3",
    "expo-constants": "~11.0.1",
    "expo-linking": "~2.3.1",
    "expo-random": "~11.1.2",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-web-browser": "~9.1.0",
    "format-json": "1.0.3",
    "formik": "2.2.9",
    "identity-obj-proxy": "3.0.0",
    "lodash": "4.17.21",
    "querystringify": "2.2.0",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-datepicker": "4.1.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-keyboard-aware-scroll-view": "0.9.4",
    "react-native-modal-datetime-picker": "10.0.0",
    "react-native-picker-select": "8.0.4",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-sectioned-multi-select": "0.8.1",
    "react-native-web": "^0.14.13",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.2.1",
    "seamless-immutable": "7.1.4",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/webpack-config": "^0.12.63",
    "@react-native-community/eslint-config": "3.0.0",
    "@storybook/addons": "5.3.21",
    "@storybook/react-native": "5.3.25",
    "@storybook/theming": "5.3.21",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "babel-jest": "26.6.3",
    "babel-preset-env": "1.7.0",
    "buffer": "6.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "expect": "26.6.2",
    "generator-jhipster-react-native": "4.2.0",
    "jest": "26.6.3",
    "jest-expo": "^42.0.0",
    "lint-staged": "11.0.0",
    "mockery": "2.1.0",
    "patch-package": "6.4.7",
    "prettier": "2.3.2",
    "redux-devtools-extension": "2.13.9",
    "rimraf": "3.0.2",
    "typescript": "4.3.4"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  }
}
